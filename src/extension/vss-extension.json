{
    "manifestVersion": 1,
    "id": "dependabot",
    "name": "Dependabot",
    "version": "0.1.0",
    "publisher": "dependabot",
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Automaticaly update dependecies and vulernatibilities in your code",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "task"
        }
    ],
    "contributions": [
        {
            "id": "dependabot",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "task"
            }
        }
    ]
}
